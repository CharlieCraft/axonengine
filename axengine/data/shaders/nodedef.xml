<?xml version="1.0" encoding="utf-8"?>
<nodeDefs>
  <node name="Time" type="uniform" datatype="float"/>
  <node name="TexCoord0" type="varying" datatype="float2"></node>
  <node name="TexCoord1" type="varying" datatype="float2"></node>
  <node name="WorldPos" type="varying" datatype="float3"></node>
  <node name="WorldNormal" type="varying" datatype="float3"></node>
  <node name="Add" type="operator" operator="+"/>
  <node name="Sub" type="operator" operator="-"/>
  <node name="Mul" type="operator" operator="*"/>
  <node name="Div" type="operator" operator="/"/>
  <node name="DotProduct" type="function" template="dot(%1, %2)"/>
  <node name="CrossProduct" type="function" template="cross(%1, %2)"/>
  <node name="Lerp" type="function" template="lerp(%1, %2, %3)"/>
  <node name="Standard" type="root">
    <socket name="diffuse" datatype="float3"/>
    <socket name="specular" datatype="float3"/>
    <socket name="normal" datatype="float3"/>
    <socket name="opacity" datatype="float"/>
    <socket name="emission" datatype="float3"/>
    <socket name="reflection" datatype="float3"/>
    <socket name="refraction" datatype="float3"/>
  </node>
</nodeDefs>